Grammar
Group Member: Lucas Menezes, Mattyws Grawe; Vitor Finati

Program -> FunctionDeclList MainFunc
FunctionDeclList -> FunctionDeclList FunctionDecl
	-> ø
FunctionDecl -> Type id (Parameters) { StmList }
Parameters -> Type id ParametersList*
	-> ø
ParametersList -> , Type id
	-> ø
MainFunc -> void main() { StmList }
StmList -> StmList Stm
	-> ø
Stm -> { Stm* }
	-> VarDecl
	-> IdStm
	-> WhileStm
	-> IfStm
VarDecl -> Type VarDeclList id DeclAssign;
VarDeclList -> VarDeclList id DeclAssign
	-> ø
DeclAssign -> = Exp
	-> ø
Type -> int 
	-> boolean
	-> float
	-> char
	-> string
	-> void
	-> int[]
	-> boolean[]
	-> float[]
	-> char[]
	-> string[]
IdStm -> id IdDef
IdDef -> = Exp;
	-> (ExpParam);
ExpParam -> ExpParamList Exp
	-> ø
ExpParamList -> ExpParamList Exp,
	-> ø
Exp -> CompareExp Clist
	-> stringValue
	-> charValue
Clist -> & CompareExp Clist
	-> | CompareExp Clist
	-> ø
CompareExp -> PlusMinusExp Plist
Plist -> < PlusMinusExp Plist
	-> < PlusMinusExp Plist
	-> == PlusMinusExp Plist
	-> <= PlusMinusExp Plist
	-> >= PlusMinusExp Plist
	-> ø
PlusMinusExp -> DivisionExp Dlist
Dlist -> + DivisionExp Dlist
	-> - DivisionExp Dlist
	-> ø
DivisionExp -> MultExp Mlist
Mlist -> / MultExp Mlist
	-> % MultExp Mlist
	-> ø
MultExp -> NotExp Nlist
Nlist -> * NotExp Nlist
	-> ø
NotExp -> ! NotExp
	-> FunctionExp ArrayExp 
ArrayExp ->[Exp] // Add tokenBracket on lexer	
	-> ø
FunctionExp -> id(ExpParam)  // conflict
	-> Factor
Factor -> number
	-> true
	-> false
	-> id
	-> Exp
WhileStm -> while (Exp) Stm
IfStm -> if (Exp) Stm ElseStm
ElseStm -> else Stm
	-> ø